<h2>Which rune page would you like to look at?</h2>
<table class="table">
  <thead>
    <th>
      Rune Page
    </th>
  </thead>
  <tbody>
    <% @rune_pages.each do |key, pages| %>
      <% pages[:pages].each do |page| %>
      <tr>
        <td>
          <%= link_to page[:name], user_rune_path(user_id: current_user.id, id: page[:id]) %>
        </td>
      </tr>
        <% end %>
    <% end %>
  </tbody>
</table>
