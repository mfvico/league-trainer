<div class="page-header">
  <div class="pull-right">
    <%= link_to "Create User", new_user_path %>
  </div>
    <h1>Users!</h1>
</div>
<table class="table">
  <thead>
    <th>IRL Name</th>
    <th>Summoner Name</th>
    <th>Email Address</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %> <%= user.last_name %></td>
      <td><%= user.summoner %></td>
      <td><%= user.email %></td>
    </tr>
    <% end %>
  </tbody>

</table>
